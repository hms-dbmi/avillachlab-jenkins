    <script>pipeline {
agent any
    
    stages {
        stage(&apos;Daily Morning Task&apos;) {
            steps {
                script {
                   echo &apos;Running morning task...&apos;
                   def result = build job: &quot;Check For Updates&quot;
                }
            }
        }
    }
}</script>
